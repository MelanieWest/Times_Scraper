 <style>
   #articles {
        display: inline-block;
        overflow: auto;
        width: 60%;
        height: 700px;
      }
      #notes {
        float: right;
        width: 30%;
      }
      textarea, input {
        display: block;
        width: 100%;
        background-color: #FCF5C7;
      }
      input {
        height: 36px;
        font-size: 24px;
      }
      textarea {
        height: 200px;
        font-size: 18px;
      }
    </style>
    <div>
    <p> this page is rendering </p>
 
  <div id ="articles" class = "col-4">

      <h4>New York Times Articles </h4>
      
      <ul>
      {{#each data }}
        <li>
          <p> <a class="delete" href="/delete/{{this._id}}" style = "color:red">Remove from list</a></p> 
          <form action = "/update/{{this._id}}" method ="POST">
            <text>{{this.title}}</text> 
            <br>      
            <text>{{this.link}}</text>       
            <button type="submit" id="read" >Read</button>,
          </form>
        </li>
      {{/each}}
      </ul>
    </div>
</div>